# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

#ignore solr
/config/blacklight.yml

#Ignore mac junk
*.DS_store

.env

# Ignore all of the reader files except the BookReader directory with the custom CSS
/public/bookreader/BookReaderDemo
/public/bookreader/BookReaderIA
/public/bookreader/bees*
/public/bookreader/bol*
/public/bookreader/hunt*

# Ignore coverage directory
coverage
