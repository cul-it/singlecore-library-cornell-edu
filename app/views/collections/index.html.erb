<h1>Collections#index</h1>
<p>Find me in app/views/collections/index.html.erb</p>

<%-  @collections.each do |collection| -%>
	<div class="media">
	  <div class="media-left">
	      <img class="media-object"  src="<%= collection.image =%>" alt="" />
	  </div>
	  <div class="media-body">
	    <div class="media-heading"><a href="<%= collection.landing %>"><%= collection.title =%></a></div>
	  </div>
	</div>
<% end %>